! Title: Cybo's Dark Mode
! Expires: 1 day
! Version: 1
! This is a userstyle for uBlock Origin that will make supported pages pitch black instead of white or grey

! Github
github.com##main:style(background-color: black !important)
github.com##div:style(background-color: black !important)
github.com##body:style(background-color: black !important)
github.com##input:style(background-color: black !important)
github.com##pre:style(background-color: grey !important)
github.com##header:style(background-color: black !important)
github.com##code:style(background-color: grey !important)
github.com##label:style(background-color: black !important)
github.com##ul:style(background-color: black !important)
github.com##li:style(background-color: black !important)
github.com##link:style(background-color: black !important)
github.com##aside:style(background-color: black !important)
github.com##td:style(background-color: black !important)
github.com##summary:style(background-color: black !important)
github.com##button:style(background-color: black !important)
github.com##div:style(color: grey !important)
github.com##label:style(color: grey !important)
github.com##summary:style(color: grey !important)
github.com##td:style(color: white !important)
github.com##input:style(color: cyan !important)
github.com##form:style(color: cyan !important)
github.com##code:style(color: cyan !important)
github.com##pre:style(color: cyan !important)
github.com##a:style(color: light-blue !important)

! FilterLists
filterlists.com##body:style(background-color: black !important)
filterlists.com##div:style(background-color: black !important)
filterlists.com##header:style(background-color: black !important)
filterlists.com##footer:style(background-color: black !important)
filterlists.com##td:style(background-color: black !important)
filterlists.com##th:style(background-color: black !important)
filterlists.com##a:style(background-color: black !important)
filterlists.com##span:style(background-color: black !important)
filterlists.com##div:style(color: grey !important)
filterlists.com##a:style(color: grey !important)
filterlists.com##span:style(color: grey !important)

! Pandora
pandora.com##body:style(background-color: black !important)
pandora.com##div:style(background-color: black !important)
pandora.com##NowPlaying__belowFold:style(background-color: black !important)
pandora.com##a:style(background-color: black !important)
pandora.com##div:style(color: grey !important)
pandora.com##h1:style(color: grey !important)
pandora.com##h2:style(color: grey !important)
pandora.com##h3:style(color: grey !important)
pandora.com##h4:style(color: grey !important)
pandora.com##h5:style(color: grey !important)
pandora.com##h6:style(color: grey !important)
pandora.com##p:style(color: grey !important)
pandora.com##a:style(color: grey !important)
pandora.com##span:style(color: grey !important)
