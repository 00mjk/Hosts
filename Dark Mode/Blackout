! Title: Cybo's Blackout
! Expires: 1 day
! Version: 3
! This is a userstyle for uBlock Origin that will make supported pages pitch black instead of white or grey

! Generic
*##main:style(background-color: black !important)
*##div:style(background-color: black !important)
*##body:style(background-color: black !important)
*##input:style(background-color: black !important)
*##pre:style(background-color: grey !important)
*##header:style(background-color: black !important)
*##code:style(background-color: grey !important)
*##br:style(background-color: grey !important)
*##label:style(background-color: black !important)
*##ul:style(background-color: black !important)
*##li:style(background-color: black !important)
*##link:style(background-color: black !important)
*##aside:style(background-color: black !important)
*##td:style(background-color: black !important)
*##summary:style(background-color: black !important)
*##button:style(background-color: black !important)
*##textarea:style(background-color: black !important)
*##a:style(background-color: black !important)
*##i:style(background-color: black !important)
*##th:style(background-color: black !important)
*##span:style(background-color: black !important)
*##header:style(background-color: black !important)
*##footer:style(background-color: black !important)
*##form:style(background-color: black !important)
*##abbr:style(background-color: black !important)
*##article:style(background-color: black !important)
*##div:style(color: grey !important)
*##p:style(color: grey !important)
*##span:style(color: grey !important)
*##label:style(color: grey !important)
*##summary:style(color: grey !important)
*##h1:style(color: grey !important)
*##h2:style(color: grey !important)
*##h3:style(color: grey !important)
*##h4:style(color: grey !important)
*##h5:style(color: grey !important)
*##h6:style(color: grey !important)
*##blockquote:style(color: grey !important)
*##td:style(color: white !important)
*##input:style(color: #5bb1cd !important)
*##form:style(color: #5bb1cd !important)
*##code:style(color: #5bb1cd !important)
*##pre:style(color: #5bb1cd !important)
*##textarea:style(color: #5bb1cd !important)
*##a:style(color: #5bb1cd !important)

! Filter Lists
filterlists.com#@#a:style(background-color: black !important)
filterlists.com#@#span:style(color: grey !important)
filterlists.com#@#li:style(background-color: black !important)
filterlists.com##span:style(color: #5bb1cd !important)
filterlists.com##span:style(color: #5bb1cd !important)
filterlists.com##li.ant-pagination-simple-pager:style(background-color: grey !important)
filterlists.com###text:style(background-color: grey !important)

